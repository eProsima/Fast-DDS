names:
    fastdds-docs:
        cmake-args:
            - "-DBUILD_DOCUMENTATION=ON"
            - "-DCOMPILE_TESTS=ON"
        ctest-args: [
                "--timeout", "500"
        ]
    googletest-distribution:
        cmake-args:
            - "-Dgtest_force_shared_crt=ON"
            - "-DBUILD_SHARED_LIBS=ON"
            - "-DBUILD_GMOCK=ON"
