                <xs:element name="case" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence minOccurs="1" maxOccurs="unbounded">
                            <xs:choice minOccurs="1">
                                <xs:element name="caseDiscriminator" minOccurs="0" maxOccurs="unbounded">
                                    <xs:complexType>
