                                            <xs:simpleType>
                                                <xs:union memberTypes="string defaultType"/>
                                            </xs:simpleType>
                                        </xs:attribute>
                                    </xs:complexType>
                                </xs:element>
                                <xs:element name="member" type="memberDcl" minOccurs="0" maxOccurs="1"/>
                            </xs:choice>
                        </xs:sequence>
