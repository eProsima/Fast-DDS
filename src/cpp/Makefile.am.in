ACLOCAL_AMFLAGS = -I m4 --install

lib_LTLIBRARIES = lib${PROJECT_NAME}.la
lib${PROJECT_NAME}_la_SOURCES = ${${PROJECT_NAME}_SOURCES_AUTOTOOLS}
lib${PROJECT_NAME}_la_LDFLAGS = -version-number ${PROJECT_VERSION_AUTOTOOLS} $(BOOST_LDFLAGS) $(BOOST_SYSTEM_LDFLAGS) $(BOOST_FILESYSTEM_LDFLAGS) $(BOOST_PROGRAM_OPTIONS_LDFLAGS) -lboost_system -lboost_thread -lboost_date_time

AM_CPPFLAGS = -DFASTRTPS_SOURCE -I../../include $(BOOST_CPPFLAGS)
