			<userTransports>
				<transport_id>eth_transport</transport_id>
			</userTransports>
			<builtin>
				<metatrafficMulticastLocatorList>
					<locator>
						<ethernet>
							<address>01.00.5E.7F.00.0F</address>
							<port>6999</port>
							<pcp>7</pcp>
							<vlan_id>255</vlan_id>
						</ethernet>
					</locator>
				</metatrafficMulticastLocatorList>
			</builtin>
		</rtps>
	</participant>
</profiles>
